{
	"extends": ["airbnb-base"],
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"plugins": ["import"],
	"parserOptions": {
		"ecmaVersion": 2020,
		"modules": true,
		"experimentalObjectRestSpread": true
	},
	"rules": {
		"no-unused-vars": 0,
		"consistent-return": 0,
		"no-underscore-dangle": 0,
		"object-curly-newline": 0,
		"implicit-arrow-linebreak": "off",
		"no-tabs": 0,
		"indent": "off"
	}
}
